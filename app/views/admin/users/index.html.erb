<div class="container">
  <div class="row">
    <div class="col">
      <h4><p><span class="p-0.5 bg-secondary"> 会員一覧</span></p></h4>
        <table class="table">
          <thead>
            <tr>
              <th>会員ID</th>
              <th>氏名</th>
              <th>メールアドレス</th>
              <th>ステータス</th>
            </tr>
          </thead>
          <% @users.each do |user|%>
          <tr>
              <td><%= user.id%></td>
              <td><%= link_to user.name,admin_user_path(user)%></td>
              <td><%= user.email%></td>
              <td><%= user.is_active%></td>
          </tr>
          <% end %>
        </table>
    </div>
  </div>
</div> 